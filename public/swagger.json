  "HourlyPowerData": {
   "allOf": [
    {
     "$ref": "#/definitions/GenericAnalysisRequest"
    },
    {
     "$ref": "#/definitions/HourlyPowerDataContent"
    }
   ]
  },
  "HourlyPowerDataContent": {
   "type": "object",
   "properties": {
